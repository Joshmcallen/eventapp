<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  </head>
  <body>


<!-- all member information sorted with a table -->
        <table class="table table-hover table-inverse">
          <thead>
            <td> Name</td>
            <td> Email</td>
            <td> Phone Number</td>
            <td></td>
          </thead>
          <tbody>
          <% members.forEach(function(member){%>
            <tr>
              <td><%=member.name%></td>
              <td><%=member.email%></td>
              <td><%=member.phone%></td>
              <td><a href="/editmember/<%=member.id %>" class="btn btn-warning">Edit</a></td>
              <td>
                <form class="" action="/memberlist/<%=member._id %>" method="POST">
                <button type="submit" name="button" class="btn btn-danger">Delete</button>
                </form>
              </td>

            </tr>
            <% });%>

          </tbody>
        </table>
<!-- end of table with member information -->

<a href="/">Main Page</a>





  </body>
</html>
